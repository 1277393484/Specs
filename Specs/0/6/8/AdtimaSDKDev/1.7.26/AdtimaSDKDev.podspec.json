{
  "name": "AdtimaSDKDev",
  "version": "1.7.26",
  "summary": "Adtima AdtimaSDK for Dev Mode",
  "description": "Adtima SDK for iOS \nDocument: https://github.com/adtima/adtima-ios-sdk/wiki",
  "homepage": "https://github.com/adtima/adtima-ios-sdk/wiki",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Khiem Nguyen": "khiemnd@vng.com.vn"
  },
  "source": {
    "git": "https://github.com/adtima/adtima-ios-sdk-dev.git",
    "tag": "1.7.26.20190927_TPL42"
  },
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "vendored_frameworks": "AdtimaSDK/Frameworks/ZAD_AdtimaMobileSDK.framework"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  },
  "frameworks": [
    "CFNetwork",
    "CoreGraphics",
    "QuartzCore",
    "StoreKit",
    "AVFoundation",
    "CoreMedia",
    "MessageUI",
    "CoreTelephony",
    "AudioToolbox",
    "AdSupport",
    "MobileCoreServices",
    "SystemConfiguration"
  ],
  "weak_frameworks": [
    "Foundation",
    "MediaPlayer",
    "UIKit",
    "WebKit",
    "CoreFoundation"
  ],
  "libraries": [
    "sqlite3.0",
    "z",
    "c++",
    "xml2"
  ],
  "dependencies": {
    "ZaloSDK": [

    ]
  },
  "default_subspecs": "All_Networks",
  "subspecs": [
    {
      "name": "Adtima"
    },
    {
      "name": "Google",
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "-DUSE_ADNETWORK_GOOGLE"
      },
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "~> 7.40.0"
        ]
      }
    },
    {
      "name": "Google+Facebook",
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "-DUSE_ADNETWORK_GOOGLE -DUSE_ADNETWORK_FACEBOOK"
      },
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "~> 7.40.0"
        ],
        "FBAudienceNetwork": [

        ]
      }
    },
    {
      "name": "All_Networks",
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "-DUSE_ADNETWORK_GOOGLE -DUSE_ADNETWORK_FACEBOOK -DUSE_ADNETWORK_ADCOLONY"
      },
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "~> 7.40.0"
        ],
        "FBAudienceNetwork": [

        ],
        "AdColony": [
          "~> 4.1.0"
        ]
      }
    }
  ]
}
