{
  "name": "ConnectedAd",
  "version": "0.1.6",
  "summary": "ConnectedAdSDK for iOS.",
  "description": "This pod is used for integrating ConnectedAdSDK in Objective-C iOS projects.",
  "homepage": "https://github.com/connsdkimpl/Connected_ObjC",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "connsdkimpl": "sdkimpl@gmail.com"
  },
  "source": {
    "git": "https://github.com/connsdkimpl/Connected_ObjC.git",
    "tag": "0.1.6"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "ConnectedSDK_ObjC/**/*.{h,m}",
  "resource_bundles": {
    "ConnectedSDK_ObjC": [
      "ConnectedSDK_ObjC/Assets/*.png"
    ]
  },
  "user_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "static_framework": true,
  "dependencies": {
    "mopub-ios-sdk/Core": ">= 5.3.0",
    "Google-Mobile-Ads-SDK": ">= 7.14.0"
  }
}
